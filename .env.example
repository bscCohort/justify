SUPABASE_INSTANCE_USER=your_user
SUPABASE_INSTANCE_PASSWORD=your_password
SUPABASE_INSTANCE_HOST=your_host
SUPABASE_INSTANCE_PORT=6543
SUPABASE_INSTANCE_DATABASE=postgres
SUPABASE_INSTANCE_POOL_MODE=transaction

SUPABASE_INSTANCE_PROJECT_ID=your_project_id

# Frontend API base (local default)
NEXT_PUBLIC_API_BASE=/api
NEXT_PUBLIC_BASE_PATH=<Github_Codespace_frontend_url>

# Proxy settings (comment out when running locally without a proxy)
# VS Code Server example:
# NEXT_PUBLIC_ASSET_PREFIX=/proxy/3002
# NEXT_PUBLIC_BASE_PATH=/proxy/3002
# NEXT_PUBLIC_API_BASE=/proxy/5000
# GitHub Codespaces example (replace <codespace-domain> and port):
# NEXT_PUBLIC_ASSET_PREFIX=https://<codespace-domain>/proxy/3002
# NEXT_PUBLIC_BASE_PATH=/proxy/3002
# NEXT_PUBLIC_API_BASE=https://<codespace-domain>/proxy/5000
